<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100" fill="none">
  <defs>
    <linearGradient id="logoGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" stop-color="#e0f2fe"/>
      <stop offset="50%" stop-color="#bae6fd"/>
      <stop offset="100%" stop-color="#7dd3fc"/>
    </linearGradient>
    <filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
      <feDropShadow dx="2" dy="2" stdDeviation="3" flood-color="#0ea5e9" flood-opacity="0.3"/>
    </filter>
  </defs>
  
  <!-- Main A shape - left leg -->
  <path d="M20 85 L45 15 Q50 5 55 15 L55 45" 
        stroke="url(#logoGradient)" 
        stroke-width="12" 
        stroke-linecap="round" 
        stroke-linejoin="round"
        fill="none"
        filter="url(#shadow)"/>
  
  <!-- Main A shape - right leg -->
  <path d="M80 85 L55 15" 
        stroke="url(#logoGradient)" 
        stroke-width="12" 
        stroke-linecap="round" 
        stroke-linejoin="round"
        fill="none"
        filter="url(#shadow)"/>
  
  <!-- A crossbar -->
  <path d="M30 60 L70 60" 
        stroke="url(#logoGradient)" 
        stroke-width="10" 
        stroke-linecap="round"
        fill="none"
        filter="url(#shadow)"/>
  
  <!-- Checkmark overlay -->
  <path d="M35 52 L48 65 L75 30" 
        stroke="#38bdf8" 
        stroke-width="10" 
        stroke-linecap="round" 
        stroke-linejoin="round"
        fill="none"
        filter="url(#shadow)"/>
</svg>
